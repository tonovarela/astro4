---
import type { User } from "../interface/users.reponse";
const response= await fetch('https://jsonplaceholder.typicode.com/users')
const users = await response.json() as User[];
---
<table>
    <tr>
    <th>
        Name
    </th>
    <th>
        UserName
    </th>
    <th>
        Email
        </th>
    </tr>
        {            
            users.map((item:User)=>            
            <tr>
                <td>{item.name}</td> 
                <td>{item.username}</td> 
                <td>{item.email}</td> 
            </tr>                         
            )

        }
</table>